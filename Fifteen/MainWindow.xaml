        <Button x:Name="b22" Grid.Column="2" Grid.Row="4" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
