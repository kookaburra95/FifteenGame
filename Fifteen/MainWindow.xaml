                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFC96489"/>
                    <GradientStop Color="#FFF7E050" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 9
        </Button>
        <Button x:Name="b12" Grid.Column="1" Grid.Row="4" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFC96489"/>
                    <GradientStop Color="#FFF7E050" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 10
