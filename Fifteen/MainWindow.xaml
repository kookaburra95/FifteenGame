                </LinearGradientBrush>
            </Button.Foreground> 15
        </Button>
        <Button x:Name="b33" Grid.Column="3" Grid.Row="5" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF539BC3"/>
                    <GradientStop Color="#FFDA9999" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground>
        </Button>
        <Label Content="00:00:00" Grid.ColumnSpan="4" Grid.Row="1" Margin="1" HorizontalAlignment="Center" Padding="0,7,0,0" FontSize="42"></Label>
        <Label x:Name="labelMoves" Content="0" Grid.ColumnSpan="4" Grid.Row="6" Margin="1" HorizontalAlignment="Center" Padding="0,7,0,0" FontSize="42"></Label>
    </Grid>
</Window>