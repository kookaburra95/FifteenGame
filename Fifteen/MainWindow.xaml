            </Button.Foreground> 5
        </Button>
        <Button x:Name="b11" Grid.Column="1" Grid.Row="3" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
