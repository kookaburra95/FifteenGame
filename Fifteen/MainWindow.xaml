<Window x:Name="fmFifteen" x:Class="Fifteen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fifteen"
        mc:Ignorable="d"
        Title="" Height="690" Width="450" FontSize="72" FontFamily="Bahnschrift Light" Background="White" Icon="/icon.ico" WindowStartupLocation="CenterScreen" Cursor="Hand" MinHeight="690" MinWidth="450">
    <Window.Resources>
        <Color x:Key="FirstRowGradient">#FF5E48BF</Color>
    </Window.Resources>
    <Grid x:Name="LayoutRoot" Background="White" Margin="1">
        <Grid.RowDefinitions>
            <RowDefinition Height="60" />
            <RowDefinition Height="60" />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Button x:Name="buttonShuffle" Grid.ColumnSpan="2" Grid.Row="7" Margin="1" BorderBrush="{x:Null}" Background="#BFFADADD" Padding="0,5,0,0" Foreground="#FFCF2361" FontSize="48" IsTabStop="False" Click="buttonShuffle_Click">
            <Button.FocusVisualStyle>
                <Style/>
            </Button.FocusVisualStyle> Shuffle
        </Button>
        <Button x:Name="buttonRecords" Grid.ColumnSpan="2" Grid.Column="2" Grid.Row="7" Margin="1" BorderBrush="{x:Null}" Background="#BF8DBDD8" Padding="0,5,0,0" Foreground="#FF215370" FontSize="48" IsTabStop="False">
            <Button.FocusVisualStyle>
                <Style/>
            </Button.FocusVisualStyle> Records
        </Button>
        <Button x:Name="buttonStart" Grid.ColumnSpan="4" Grid.Row="0" Margin="1" BorderBrush="{x:Null}" Padding="0,0,0,0" Foreground="#FF229C2D" FontSize="48" IsTabStop="False" Click="buttonStart_Click">
            <Button.Background>
                <SolidColorBrush Color="#BFB2CFBD"/>
            </Button.Background>
            <Button.FocusVisualStyle>
                <Style/>
            </Button.FocusVisualStyle> Start
        </Button>
        <Button x:Name="b00" Grid.Column="0" Grid.Row="2" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.FocusVisualStyle>
                <Style/>
            </Button.FocusVisualStyle>
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFB82ACF"/>
                    <GradientStop Color="{DynamicResource FirstRowGradient}" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 1
        </Button>
        <Button x:Name="b10" Grid.Column="1" Grid.Row="2" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFB82ACF"/>
                    <GradientStop Color="{DynamicResource FirstRowGradient}" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 2
        </Button>
        <Button x:Name="b20" Grid.Column="2" Grid.Row="2" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
