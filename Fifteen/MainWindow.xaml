            </Button.Foreground> 4
        </Button>
        <Button x:Name="b01" Grid.Column="0" Grid.Row="3" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF0E3E83"/>
                    <GradientStop Color="#FF59CF69" Offset="1"/>
