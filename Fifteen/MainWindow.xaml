                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF0E3E83"/>
                    <GradientStop Color="#FF59CF69" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 7
        </Button>
        <Button x:Name="b31" Grid.Column="3" Grid.Row="3" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
