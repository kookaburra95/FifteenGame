                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF539BC3"/>
                    <GradientStop Color="#FFDA9999" Offset="0.993"/>
                </LinearGradientBrush>
            </Button.Foreground> 13
        </Button>
        <Button x:Name="b13" Grid.Column="1" Grid.Row="5" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF539BC3"/>
                    <GradientStop Color="#FFDA9999" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 14
        </Button>
        <Button x:Name="b23" Grid.Column="2" Grid.Row="5" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
            <Button.Foreground>
