                    <GradientStop Color="#FFB82ACF"/>
                    <GradientStop Color="{DynamicResource FirstRowGradient}" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> 3
        </Button>
        <Button x:Name="b30" Grid.Column="3" Grid.Row="2" Margin="1" BorderBrush="{x:Null}" Background="#7FDDDDDD" Padding="0,12,0,0" IsTabStop="False" Click="b_Click">
